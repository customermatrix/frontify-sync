{
  "name": "frontify-sync",
  "version": "0.1.1",
  "description": "Frontify-sync provides automatic synchronization between local patterns and assets folders to a Frontify project",
  "keywords": [
    "frontify",
    "patterns",
    "assets",
    "synchronization"
  ],
  "url": "https://github.com/customermatrix/frontify-sync",
  "bugs": "https://github.com/customermatrix/frontify-sync/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/customermatrix/frontify-sync.git"
  },
  "license": "MIT",
  "preferGlobal": true,
  "scripts": {
    "test": "istanbul cover _mocha",
    "test-ci": "mocha --watch",
    "release": "standard-version",
    "publish": "git push --follow-tags origin master && npm publish"
  },
  "main": "./src/main.js",
  "bin": "./index.js",
  "dependencies": {
    "@frontify/frontify-api": "0.3.1",
    "lodash": "4.16.3",
    "cli-color": "1.1.0",
    "bluebird": "3.4.6"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-subset": "1.3.0",
    "chai-as-promised": "6.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.1.2",
    "sinon": "1.17.6",
    "standard-version": "3.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
